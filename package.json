{
  "name": "spellbook",
  "version": "1.0.0",
  "description": "VectorDB-based memory MCP server for AI agents",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "spellbook": "src/index.ts"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "seed": "bun run src/scripts/seed-system-guides.ts",
    "typecheck": "tsc --noEmit",
    "build": "bun build src/index.ts --outdir=dist --target=bun"
  },
  "keywords": [
    "mcp",
    "vectordb",
    "qdrant",
    "memory",
    "ai-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@qdrant/js-client-rest": "^1.11.0",
    "axios": "^1.7.0",
    "express": "^4.21.2",
    "uuid": "^11.0.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.7.2"
  }
}
